/* pages/about/index.wxss */
page{
    background: #F8F8F8;
    color: #333;
}
.icoBox,.icoInfoBox{
    background: #FFF;
    text-align: center;
    padding: 0 0 20rpx 0;
}
.icoBox .ico{
    width: 240rpx;
}
.icoInfoBox .info{
    font-size: 28rpx;
}
.icoInfoBox .share{
    margin: 30rpx auto;
    font-size: 26rpx;
}


.pageTitle{
    padding: 20px;
    font-size: 20px;
    font-weight: 400;
}
.cells{
    background-color: #FFFFFF;
    line-height: 1.47058824;
    font-size: 17px;
    overflow: hidden;
    position: relative;
}
.cells:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;
}
.cells:after,.cell:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;
}
.cells .cell{
    padding: 10px 15px;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    font-size: 30rpx;
}
.cells .cell.ceilico:after{
    content: " ";
    display: inline-block;
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #C8C8CD;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    position: absolute;
    top: 50%;
    margin-top: -8px;
    right: 20px;
    left: auto;
}
.cells .cell .info{
    flex: 1;
    text-align: right;
    color: #999999;
}
.cells .cell .info.f-link{
    color: #10aeff;
}
.itemIco{
    display: block;
    width: 42rpx;
}
.footer {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 60rpx;
  font-size: 22rpx;
  color: #333;
}